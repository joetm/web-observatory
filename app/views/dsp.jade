doctype html
html(lang='en')
  include includes/head
  script(src='/js/jquery.dataTables.js')
  link(href='/css/jquery.dataTables.css', rel='stylesheet')

  body
    #wrap
      .container.text-center
        .row
          include includes/flash-banner
        .row
          table#display.table.table-bordered.table-hover
            if result
              thead
                tr
                  each v in result.head.vars
                    th=v
              tbody
                each binding in result.results.bindings
                  tr
                    each val in binding
                      td=val.value
      #push
  include includes/footer
  script.
    $(document).ready(
    function(){
      $('#display').dataTable();
    });
